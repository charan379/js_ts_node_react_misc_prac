{
  "name": "ServiceTool",
  "version": "0.9.130",
  "description": "Enduser software to find shared resources.",
  "main": "main.js",
  "productCode": "v",
  "author": "Conguide GmbH",
  "scripts": {
    "start": "NODE_ENV=development electron ./",
    "startwin": "electron ./",
    "fixkeytar": "$(npm bin)/electron-rebuild",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "distwin": "yarn version --patch && electron-builder -w",
    "distwinnobump": "electron-builder -w",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "Service Tool",
    "asarUnpack": [
      "./node_modules/node-notifier/vendor/**"
    ],
    "extraResources": [
      "./psscripts",
      "./env-variables.json",
      "./locales"
    ],
    "files": [
      "!**/.vscode/${/*}",
      "!**/node_modules/node-notifier/vendor/mac.noindex/${/*}"
    ],
    "appId": "3d717368-dc0c-4ed3-88be-d914419bfa78",
    "icon": "./main/appicon/png/1024x1024.png",
    "buildDependenciesFromSource": false,
    "linux": {
      "category": "productivity",
      "target": [
        "AppImage",
        "Zip"
      ]
    },
    "portable": {
      "unpackDirName": "servicetoolportable"
    },
    "msi": {
      "oneClick": true,
      "perMachine": true,
      "createDesktopShortcut": false,
      "runAfterFinish": false,
      "artifactName": "Service Tool Setup ${version}.${ext}"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": false,
      "runAfterFinish": true,
      "createDesktopShortcut": false,
      "artifactName": "Service Tool Setup ${version}.${ext}"
    },
    "win": {
      "publisherName": "Conguide GmbH",
      "target": [
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64"
          ]
        }
      ]
    }
  },
  "keywords": [],
  "license": "ISC",
  "devDependencies": {
    "electron": "^13.5.2",
    "electron-builder": "^22.13.1",
    "electron-rebuild": "^3.2.3"
  },
  "dependencies": {
    "@pnp/common-commonjs": "^2.10.0",
    "@pnp/logging-commonjs": "^2.10.0",
    "@pnp/nodejs-commonjs": "^2.10.0",
    "@pnp/odata-commonjs": "^2.10.0",
    "@pnp/sp-commonjs": "^2.10.0",
    "auto-launch": "^5.0.5",
    "axios": "^0.24.0",
    "i18next": "^21.3.3",
    "i18next-fs-backend": "^1.1.1",
    "i18next-http-backend": "^1.3.1",
    "jwt-decode": "^3.1.2",
    "keytar": "^7.7.0",
    "moment": "^2.24.0",
    "node-notifier": "10.0.0",
    "node-powershell": "^4.0.0",
    "os-locale": "^5.0.0"
  }
}
